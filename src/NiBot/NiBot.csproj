<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <Version>8.0.7</Version>
    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>nibot</AssemblyName>
    <RootNamespace>Aiursoft.NiBot</RootNamespace>
    <IsTestProject>false</IsTestProject>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>nibot</ToolCommandName>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <SelfContained>false</SelfContained>
    <PublishTrimmed>false</PublishTrimmed>
    <PublishReadyToRun>false</PublishReadyToRun>
    <PublishSingleFile>false</PublishSingleFile>
    <Company>Aiursoft</Company>
    <Product>NiBot</Product>
    <Authors>edgeneko anduin2017</Authors>
    <Description>A cli tool helps you to de-duplicate images in a folder.</Description>
    <PackageId>Aiursoft.NiBot</PackageId>
    <PackageTags>cli nuget dotnet tool image deduplication</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://gitlab.aiursoft.cn/Aiursoft/NiBot</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://gitlab.aiursoft.cn/Aiursoft/NiBot</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\NiBot.Calendar\NiBot.Calendar.csproj" />
    <None Include="../../README.md" Pack="true" PackagePath="." />
    <ProjectReference Include="..\NiBot.Dedup\NiBot.Dedup.csproj" />
  </ItemGroup>
</Project>